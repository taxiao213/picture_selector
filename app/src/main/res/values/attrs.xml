    <attr name="picture_selector_slide_text_size" format="dimension" />                  <!--滑动显示的文字大小-->
    <attr name="picture_selector_slide_text_color" format="reference|color" />           <!--滑动显示的文字颜色-->

    <!--recyclerview-->
    <attr name="picture_selector_ry_bg" format="reference|color" />                      <!--recyclerview背景色-->
    <attr name="picture_selector_ry_checkbox" format="reference" />                      <!--checkbox样式-->
    <attr name="picture_selector_ry_checkbox_animation" format="reference" />            <!--checkbox样式-->

    <!--预览界面底部布局-->
    <attr name="picture_selector_previews_ll_bg" format="reference|color" />             <!--预览界面底部背景色-->

    <!--编辑界面顶部-->
    <attr name="picture_edit_top_cancel_text_content" format="string" />                 <!--编辑界面顶部取消文字内容-->
    <attr name="picture_edit_top_cancel_text_color" format="reference|color" />          <!--编辑界面顶部取消文字颜色-->
    <attr name="picture_edit_top_complete_text_content" format="string" />               <!--编辑界面顶部完成文字内容-->
    <attr name="picture_edit_top_complete_text_color" format="reference|color" />        <!--编辑界面顶部完成字体颜色-->
    <attr name="picture_edit_top_text_size" format="dimension" />                        <!--编辑界面顶部文字大小-->
    <attr name="picture_edit_bottom_pencile" format="reference" />                       <!--编辑界面底部pencile图标-->
    <attr name="picture_edit_bottom_text" format="reference" />                          <!--编辑界面底部text图标-->
    <attr name="picture_edit_bottom_mosaic" format="reference" />                        <!--编辑界面底部mosaic图标-->
    <attr name="picture_edit_bottom_cut" format="reference" />                           <!--编辑界面底部cut图标-->
    <attr name="picture_edit_bottom_mosaic_grid" format="reference" />                   <!--编辑界面底部mosaic_grid图标-->
    <attr name="picture_edit_bottom_mosaic_sand" format="reference" />                   <!--编辑界面底部mosaic_sand图标-->
    <attr name="picture_edit_bottom_withdraw" format="reference" />                      <!--编辑界面底部withdraw图标-->
    <attr name="picture_edit_text_bold" format="reference" />                            <!--编辑界面text文字加粗图标-->

    <!--圆形view-->
    <declare-styleable name="RoundView">
        <attr name="round_radius" format="float"/>                                       <!--半径-->
        <attr name="round_spacing" format="float"/>                                      <!--间距-->
        <attr name="round_scale_coefficient" format="float"/>                            <!--缩放系数-->
        <attr name="round_back_color" format="color"/>                                   <!--后背景色-->
        <attr name="round_front_color" format="color"/>                                  <!--前背景色-->
    </declare-styleable>

</resources>