    <attr name="picture_selector_bottom_center_text_size" format="dimension" />          <!--中间原图文字大小-->
    <attr name="picture_selector_bottom_center_text_color" format="reference|color" />   <!--中间原图文字颜色-->
    <attr name="picture_selector_bottom_center_text_drawable_left" format="reference" /> <!--中间原图文字背景-->
    <attr name="picture_selector_bottom_preview_text_size" format="dimension" />         <!--右侧预览文字大小-->
    <attr name="picture_selector_bottom_preview_text_color" format="reference|color" />  <!--右侧预览文字颜色-->

    <!--滑动显示的时间状态-->
    <attr name="picture_selector_slide_height" format="dimension" />                     <!--滑动显示的高度-->
